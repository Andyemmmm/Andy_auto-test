		{
			"name": "登录请求",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "39fa849d-26f7-4c1e-8a5a-9dc075f31f72",
						"exec": [
							"var random = pm.environment.get(\"random\"); ",
							"",
							"pm.environment.set(\"account\", data.account);",
							"pm.environment.set(\"password\", data.password);",
							"",
							"var password = pm.environment.get(\"password\"); ",
							"var account = pm.environment.get(\"account\"); ",
							"",
							"// 加密",
							"var pwd1 = CryptoJS.MD5(password).toString();",
							"var pwd2 = CryptoJS.MD5(pwd1 + account).toString();",
							"var pwd3 = CryptoJS.MD5(pwd2 + random).toString();",
							"",
							"// 存入环境变量",
							"pm.environment.set(\"rawPassword\", pwd1);",
							"pm.environment.set(\"pwd3\", pwd3);"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "40f92f47-f719-4f9e-bb62-bdb9b6e02f3e",
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"pm.test(\"Your test name\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.result).to.eql(\"success\");",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
