						"exec": [
							"// v.random = \"4afd95216c490a2696b31c0239e23547\";",
							"// 通过正则表达式从响应中获取随机字符串",
							"var random = pm.response.text().match('v.random = \"(.*?)\";')[1];",
							"console.log(random);",
							"",
							"// 将随机值存入环境变量中",
							"pm.environment.set(\"random\", random);",
							""
